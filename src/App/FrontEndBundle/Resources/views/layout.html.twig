<html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no"/>

    <meta name="keywords" content="{% block metaKeywords %}{% endblock %}">
    <meta name="description" content="{% block metaDescription %}{% endblock %}">

    <title>{% block pageTitle %}MediaMonks Symfony Skeleton{% endblock %}</title>

    <meta name="document-base"
          content="{{ app.request.getScheme() }}://{{ app.request.getHost() }}{{ app.request.baseUrl }}/"/>

    <base href="{{ app.request.getScheme() }}://{{ app.request.getHost() }}{{ app.request.baseUrl }}/version/{{ asset_version }}/"/>

    <meta property="og:url"
          content="{{ app.request.getScheme() }}://{{ app.request.getHost() }}{{ app.request.baseUrl }}{{ app.request.pathInfo }}/"/>
    <meta property="og:type" content="website"/>
    <meta property="og:title" content="{{ block('pageTitle') }}"/>
    <meta property="og:description" content="{{ block('metaDescription') }}"/>
    <meta property="og:image" content=""/>
    <meta property="fb:app_id" content=""/>

    <!--
    <link href="inc/style/screen.css" rel="stylesheet" type="text/css" />
    <script src="inc/script/lib/modernizr/modernizr.js"></script>
    <link rel="shortcut icon" href="inc/image/favicon.ico" type="image/x-icon" />
    -->
</head>
<body data-gaia-container="main">

