{
    "name": "mediamonks/skeleton-symfony",
    "description": "MediaMonks Symfony Skeleton Project",
    "type": "project",
    "license": "proprietary",
    "homepage": "https://www.mediamonks.com/",
    "authors": [
        {
            "name": "Robert Slootjes",
            "email": "robert@mediamonks.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/",
            "App\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php",
            "app/Environment.php"
        ]
    },
    "require": {
        "php": "^5.6|^7.0",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "mediamonks/rest-api-bundle": "^3.0",
        "nelmio/api-doc-bundle": "^2.11",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "doctrine/data-fixtures": "^1.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "mediamonks/doctrine-extensions": "^1.0",
        "zendframework/zend-crypt": "^2.6",
        "ocramius/proxy-manager": "~1.0",
        "sonata-project/doctrine-orm-admin-bundle": "~3.0",
        "sonata-project/admin-bundle": "~3.0",
        "firebase/php-jwt": "^3.0",
        "wikimedia/ip-set": "^1.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "composer/composer": "^1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "hard-copy"
    }
}
