{
    "type": "project",
    "name": "mediamonks/skeleton",
    "description": "MediaMonks Skeleton",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "symfony/console": "*",
        "symfony/flex": "^1.3.1",
        "symfony/runtime": "*",
        "symfony/framework-bundle": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.93",
        "phpstan/phpstan-symfony": "^0.12.41"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "security-check": [
            "symfony check:security"
        ],
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@security-check"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@security-check"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "",
            "allow-contrib": false,
            "require": "5.3.*",
            "endpoint": "https://symfony.mediamonks.net"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://toran.mediamonks.net/repo/private/"
        }
    ]
}
